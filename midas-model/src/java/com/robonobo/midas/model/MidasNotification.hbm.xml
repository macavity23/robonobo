<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.robonobo.midas.model">
	<class name="MidasNotification" table="notifications">
        <id name="notifId" type="long">
        	<generator class="native"/>
        </id>
        <property name="updateUserId"/>
        <property name="notifUserId"/>
        <property name="item"/>
        <property name="date"/>
    </class>
</hibernate-mapping>
